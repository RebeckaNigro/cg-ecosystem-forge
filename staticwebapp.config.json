{
  "globalHeaders": {
    "content-security-policy": "img-src 'self' data:; default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/img/*.{png,jpg,gif,webp}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".html": "text/html"
  }
}
